INPUT a, b
OUTPUT o
VAR a:4, b:4, _l_2:4, c:4, _c_sel_0, _l_5, _l_6, _c_sel_1, _l_8, _c_sel_2, _l_10, _c_sel_3, o
IN
_l_2 = XOR a b
c = NOT _l_2
_c_sel_0 = SELECT 0 c
_l_5 = SELECT 0 c
_l_6 = AND _c_sel_0 _l_5
_c_sel_1 = SELECT 1 c
_l_8 = AND _l_6 _c_sel_1
_c_sel_2 = SELECT 2 c
_l_10 = AND _l_8 _c_sel_2
_c_sel_3 = SELECT 3 c
o = AND _l_10 _c_sel_3
